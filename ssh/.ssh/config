Host *
    CheckHostIP no
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%h:%p
    ControlPersist yes
    ServerAliveInterval 30
    StrictHostKeyChecking no
    TCPKeepAlive yes
    ForwardAgent yes
    ForwardX11 no
    HashKnownHosts no
    PermitLocalCommand yes
Host dev-cn
    HostName 10.227.94.22
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials no
Host *.github.com
    AddKeysToAgent yes
    IdentityFile ~/.ssh/github_key
